{
  "name": "docker-php-laravel-ci",
  "private": true,
  "version": "1.0.0",
  "description": "Docker image for Laravel stack (for CI environment)",
  "wf-docker": {
    "image": "pionl/docker-php-laravel-ci",
    "tags": [
      {
        "tag": "8.2-node-20",
        "docker": {
          "php": "8.2",
          "node": "20.9.0",
          "npm": "10.2",
          "composer": "2.7.2"
        }
      },
      {
        "tag": "8.2-node-18",
        "docker": {
          "php": "8.2",
          "node": "18.19.1",
          "npm": "9.6",
          "composer": "2.5.5"
        }
      },
      {
        "tag": "8.1-node-18",
        "docker": {
          "php": "8.1",
          "node": "18.19.1",
          "npm": "9.6",
          "composer": "2.5.5"
        }
      },
      {
        "tag": "8.1-node-17",
        "docker": {
          "php": "8.1",
          "node": "17.9.1",
          "npm": "8.1.3",
          "composer": "2.1.14"
        }
      },
      {
        "tag": "8.0-node-17",
        "docker": {
          "php": "8.0",
          "node": "17.9.1",
          "npm": "8.1.3",
          "composer": "2.1.14"
        }
      }
    ],
    "run": [
      "node --version",
      "npm --version",
      "php --version"
    ]
  },
  "scripts": {
    "build": "./node_modules/.bin/wf-docker build",
    "build-push": "./node_modules/.bin/wf-docker build-push",
    "push": "./node_modules/.bin/wf-docker push"
  },
  "author": "Martin Kluska <martin@kluska.cz",
  "license": "MIT",
  "dependencies": {
    "wf-docker": "1.0.3"
  }
}
