{
  "name": "docker-php-laravel-ci",
  "private": true,
  "version": "1.0.0",
  "description": "Docker image for Laravel stack (for CI environment)",
  "wf-docker": {
    "image": "pionl/docker-php-laravel-ci",
    "tags": [
      {
        "tag": "8.4-node-22",
        "docker": {
          "php": "8.4",
          "node": "22.14.0",
          "npm": "11.2.0",
          "composer": "2.8.6"
        }
      },
      {
        "tag": "8.3-node-22",
        "docker": {
          "php": "8.3",
          "node": "22.14.0",
          "npm": "11.2.0",
          "composer": "2.8.6"
        }
      },
      {
        "tag": "8.3-node-20",
        "docker": {
          "php": "8.3",
          "node": "20.9.0",
          "npm": "10.2",
          "composer": "2.8.6"
        }
      }
    ],
    "run": [
      "node --version",
      "npm --version",
      "php --version"
    ]
  },
  "scripts": {
    "build": "./node_modules/.bin/wf-docker build",
    "build-push": "./node_modules/.bin/wf-docker build-push",
    "push": "./node_modules/.bin/wf-docker push"
  },
  "author": "Martin Kluska <martin@kluska.cz",
  "license": "MIT",
  "dependencies": {
    "wf-docker": "1.0.3"
  }
}
